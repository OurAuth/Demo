{
    "Serilog": {
        "MinimumLevel": {
            "Default": "Error",
            "Override": {
                "OurAuth.Demo": "Information"
            }
        },
        "WriteTo": [
            {
                "Name": "Console"
            },
            {
                "Name": "File",
              "Args": {
                "path": "Logs\\app.log",
                "rollingInterval": "Day"
              }
            }
        ]
    }
}